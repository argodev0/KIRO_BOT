{
  "name": "ai-crypto-trading-bot",
  "version": "1.0.0",
  "description": "AI-powered cryptocurrency trading bot with advanced technical analysis",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && vite build",
    "build:backend": "tsc",
    "build:frontend": "vite build",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "dev:frontend": "vite",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern='__tests__/(services|models|utils|validation)' --runInBand",
    "test:integration": "jest --testPathPattern='__tests__/integration' --runInBand",
    "test:e2e": "jest --testPathPattern='__tests__/e2e' --runInBand",
    "test:frontend": "jest --testPathPattern=src/frontend --runInBand",
    "test:security": "jest --testPathPattern='__tests__/security' --runInBand",
    "test:performance": "jest --testPathPattern='__tests__/performance' --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "test:ci": "jest --ci --coverage --watchAll=false --runInBand",
    "test:load": "node scripts/load-test.js",
    "test:benchmark": "node scripts/benchmark.js",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix",
    "quality:check": "npm run lint && npm run test:coverage && npm run test:security",
    "migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio"
  },
  "keywords": [
    "crypto",
    "trading",
    "bot",
    "ai",
    "technical-analysis"
  ],
  "author": "AI Crypto Trading Bot",
  "license": "MIT",
  "dependencies": {
    "@elastic/elasticsearch": "^8.11.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/x-charts": "^8.10.1",
    "@mui/x-data-grid": "^8.10.1",
    "@prisma/client": "^5.6.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/react-router-dom": "^5.3.3",
    "amqplib": "^0.10.3",
    "axios": "^1.6.0",
    "bcryptjs": "^2.4.3",
    "charting_library": "^1.0.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "prom-client": "^15.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "recharts": "^3.1.2",
    "redis": "^4.6.10",
    "socket.io": "^4.7.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "twilio": "^4.19.0",
    "web-push": "^3.6.6",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/amqplib": "^0.10.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/socket.io-client": "^1.4.36",
    "@types/supertest": "^2.0.16",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/web-push": "^3.6.3",
    "@types/ws": "^8.5.8",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^5.0.1",
    "artillery": "^2.0.0",
    "eslint": "^8.53.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-html-reporters": "^3.1.7",
    "nock": "^13.4.0",
    "playwright": "^1.40.0",
    "prisma": "^5.6.0",
    "socket.io-client": "^4.8.1",
    "supertest": "^6.3.3",
    "testcontainers": "^10.4.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "vite": "^7.1.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
